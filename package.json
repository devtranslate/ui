{
  "name": "@devtranslate/ui",
  "version": "0.0.1",
  "description": "Devtranslate UI Components",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "scripts": {
    "prepare": "husky",
    "build": "rollup -c",
    "lint:code": "tsc --noEmit && eslint . && prettier --check .",
    "lint:fix": "tsc --noEmit && eslint --fix . && prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devtranslate/ui.git"
  },
  "keywords": [],
  "author": "Larissa Mourullo",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/devtranslate/ui/issues"
  },
  "homepage": "https://github.com/devtranslate/ui#readme",
  "peerDependencies": {
    "react": ">=16.8"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "commitizen": "^4.3.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    ".{ts,tsx}": "eslint --fix",
    ".{ts,tsx,css}": "prettier --write"
  },
  "pre-commit": [
    "npm prettier-format",
    "npm lint"
  ]
}
